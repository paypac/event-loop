{
  "name": "slownode",
  "version": "0.4.0",
  "description": "slownode",
  "readmeFilename": "README.md",
  "main": "built/index.js",
  "typings": "built/index.d.ts",
  "scripts": {
    "build": "npm run build-ts && npm run build-peg && npm run build-copy",
    "build-ts": "tsc -p src && tsc -p test && tsc -p www",
    "build-peg": "pegjs src/converters/jasm-to-stepper/jasm-grammar.peg built/converters/jasm-to-stepper/jasm-grammar.js",
    "build-copy": "ncp built/converters/jasm-to-stepper/jasm-grammar.js www/built/converters/jasm-to-stepper/jasm-grammar.js  && ncp test/fixtures/ built/test/fixtures/",

    "clean": "rimraf built/",
    "prepublish": "npm run self-ref && npm run build",
    "test": "node ./scripts/run-unit-tests",
    "self-ref": "node ./scripts/enable-self-reference"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paypac/slownode"
  },
  "keywords": [
    "persisent",
    "event",
    "loop",
    "emitter",
    "promise",
    "slow",
    "function"
  ],
  "contributors": [
    {
      "name": "Troy Gerwien",
      "email": "yortus@gmail.com"
    },
    {
      "name": "Carl Winkler",
      "email": "carl@carlossus.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paypac/slownode/issues"
  },
  "homepage": "https://github.com/paypac/slownode",
  "dependencies": {
    "babel-standalone": "^6.7.7",
    "typescript": "^1.9.0-dev.20160619-1.0"
  },
  "devDependencies": {
    "@types/babel-core": "^6.7.8-alpha",
    "@types/babel-types": "^6.7.8-alpha",
    "@types/chai": "^3.4.22-alpha",
    "@types/mocha": "^2.2.21-alpha",
    "@types/node": "^4.0.22-alpha",
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "pegjs": "^0.9.0",
    "ncp": "^2.0.0",
    "rimraf": "^2.5.2"
  }
}
